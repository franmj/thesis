journal = {Journal of molecular cell biology},
language = {en},
month = {aug},
number = {4},
pages = {197--206},
pmid = {22661322},
publisher = {Oxford University Press},
title = {{Rewiring drug-activated p53-regulatory network from suppressing to promoting tumorigenesis.}},
