issn = {1553-7358},
journal = {PLoS computational biology},
month = {apr},
number = {4},
pages = {e1003571},
pmid = {24722481},
title = {{rDock: A Fast, Versatile and Open Source Program for Docking Ligands to Proteins and Nucleic Acids.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3983074{\&}tool=pmcentrez{\&}rendertype=abstract},
