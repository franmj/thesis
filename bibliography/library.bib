issn = {1543-8392},
journal = {Molecular pharmaceutics},
keywords = {computational alanine scanning,homology model,mm,pbsa,per-residue free energy decomposition,pharmacophore model,$\sigma$ 1 receptor},
month = {oct},
pmid = {23020867},
title = {{Another Brick in the Wall. Validation of the $\sigma$(1) Receptor 3D Model by Computer-Assisted Design, Synthesis, and Activity of New $\sigma$(1) Ligands.}},
