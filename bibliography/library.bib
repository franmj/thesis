keywords = {Amino Acid Substitution,Animals,Computational Biology,Computer Simulation,Databases, Protein,Evolution, Molecular,Humans,Models, Molecular,Point Mutation,Protein Conformation,Proteins,Proteins: chemistry,Proteins: genetics},
