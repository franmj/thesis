isbn = {8862235828},
issn = {10780432},
journal = {Clinical Cancer Research},
number = {15},
pages = {4877},
pmid = {18676761},
title = {{Lung cancer with epidermal growth factor receptor exon 20 mutations is associated with poor gefitinib treatment response}},
volume = {14},
year = {2008}
}
