journal = {Current protocols in protein science / editorial board, John E. Coligan ... [et al.]},
language = {en},
month = {dec},
pages = {Unit 2.9},
pmid = {18429317},
publisher = {John Wiley {\&} Sons, Inc.},
title = {{Comparative protein structure modeling using MODELLER.}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/0471140864.ps0209s50/full},
volume = {Chapter 2},
year = {2007}
