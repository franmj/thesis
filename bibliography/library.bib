doi = {10.1038/clpt.2013.176},
file = {:home/fran/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Harrold, Ramanathan, Mager - 2013 - Network-Based Approaches in Drug Discovery and Early Development.pdf:pdf},
issn = {1532-6535},
journal = {Clinical pharmacology and therapeutics},
month = {sep},
