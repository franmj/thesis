file = {:home/fran/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yamanishi et al. - 2011 - Extracting sets of chemical substructures and protein domains governing drug-target interactions.pdf:pdf},
issn = {1549-960X},
journal = {Journal of chemical information and modeling},
