title = {{SpecNet: a spatial network algorithm that generates a wide range of specific structures.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3411677{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
