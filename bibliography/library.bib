journal = {Nature chemical biology},
keywords = {Animals,Antineoplastic Agents,Antineoplastic Agents: chemistry,Antineoplastic Agents: therapeutic use,Antineoplastic Agents: toxicity,Drug Delivery Systems,Drug Design,Genomics,Humans,Pharmacology,Pharmacology: methods,Pharmacology: trends,Structure-Activity Relationship,Systems Biology},
