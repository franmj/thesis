title = {{Evolutionary dynamics of cancer in response to targeted combination therapy.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3691570{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {2},
year = {2013}
}
