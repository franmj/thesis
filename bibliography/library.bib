journal = {Nucleic acids research},
month = {may},
pages = {1--7},
pmid = {24838565},
title = {{DINIES: drug-target interaction network inference engine based on supervised analysis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24838565},
year = {2014}
}
