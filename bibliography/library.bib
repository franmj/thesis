journal = {Analytica chimica acta},
month = {nov},
pages = {1--10},
pmid = {23101647},
title = {{Large-scale prediction of drug-target interactions using protein sequences and drug topological structures.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23101647},
volume = {752},
year = {2012}
}
