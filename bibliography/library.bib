file = {:home/fran/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pal et al. - 2008 - Juxtaposition of the steroid binding domain-like I and II regions constitutes a ligand binding site in the sigma-1 r.pdf:pdf},
isbn = {6082625382},
