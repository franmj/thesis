journal = {Nature biotechnology},
keywords = {Computational Biology,Computational Biology: methods,Genetic Diseases, Inborn,Genetic Diseases, Inborn: genetics,Humans,Image Processing, Computer-Assisted,Metabolic Networks and Pathways,Metabolic Networks and Pathways: genetics,Mutation,Protein Interaction Maps},
month = {mar},
