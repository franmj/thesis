pmid = {23733187},
title = {{A novel family of soluble minimal scaffolds provides structural insight into the catalytic domains of integral membrane metallopeptidases.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23733187},
volume = {288},
year = {2013}
}
