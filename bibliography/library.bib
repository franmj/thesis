file = {:home/fran/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Beau-Faller et al. - 2014 - Rare EGFR exon 18 and exon 20 mutations in non-small-cell lung cancer on 10 117 patients A multicentre obser.pdf:pdf},
